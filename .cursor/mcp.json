{
  "mcpServers": {
    "mcp-installer": {
      "command": "npx",
      "args": [
        "@anaisbetts/mcp-installer"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-filesystem",
        "."
      ]
    },
    "commands": {
      "command": "npx",
      "args": [
        "mcp-server-commands"
      ]
    },
    "nodejs-debugger": {
      "command": "npx",
      "args": [
        "@hyperdrive-eng/mcp-nodejs-debugger"
      ]
    },
    "inspector": {
      "command": "npx",
      "args": [
        "mcp-inspector"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": "/Users/samdarkwa/Documents/cursor-workspaces/suffolk-chatbot/memory.json"
      }
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "postgresql://localhost/suffolk_chatbot"
      ]
    },
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "sbp_4a1a1b8afe49813e71a42b37fb6006c1603d9600"
      ]
    },
    "airtable-felores": {
      "command": "npx",
      "args": [
        "-y",
        "@felores/airtable-mcp-server"
      ],
      "env": {
        "AIRTABLE_API_KEY": "$env:AIRTABLE_API_KEY"
      }
    },
    "Linear": {
      "url": "https://mcp.linear.app/sse",
      "headers": {
        "Authorization": "Bearer $env:LINEAR_API_KEY"
      }
    },
    "Notion": {
      "url": "https://mcp.notion.com/mcp",
      "headers": {
        "Authorization": "Bearer $env:NOTION_API_KEY"
      }
    }
  }
}
